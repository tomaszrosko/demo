root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = false
max_line_length = 120
tab_width = 4

[*.scss]
indent_size = 4
tab_width = 4

[*.sass]
indent_size = 4

[{*.cjs,*.js}]
max_line_length = 80

[{*.htm,*.html,*.ng,*.sht,*.shtm,*.shtml}]
max_line_length = 160
